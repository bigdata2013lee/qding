<div class="query_box" data-template="query_box">
    <h4>升级管理</h4>
    <div class="modal fade" id="upgrade_window" tabindex="-1" role="dialog">
        <div class="modal-dialog bs-example-modal-sm" role="document">
            <div class="modal-content">

                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title">发布新版本</h4>
                    <div class="progress" style="display: none;margin: 0;">
                        <div class="progress-bar progress-bar-info progress-bar-striped active" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0;" id="percent">
                            <span></span>
                        </div>
                    </div>
                </div>

                <div class="modal-body" data-template="alarm_areas_window_body">
                    <div class="view_body">
                        <form id="agw_release_form" method="post" class="form-horizontal" enctype="multipart/form-data">

                            <div class="form-group">
                                <label class="col-sm-3 control-label">文件类型</label>
                                <div class="col-sm-9">
                                    <select name="type_name" class="form-control">
                                        <option value="sw">sw</option>
                                        <option value="res">res</option>
                                        <option value="wifi">wifi</option>
                                    </select>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-sm-3 control-label">版本号</label>
                                <div class="col-sm-9">
                                    <input name="version" type="text" class="form-control"/>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-sm-3 control-label" style="padding-top: 2px;">上传升级包</label>
                                <div class="col-sm-9">
                                    <input name="bin_file" type="file" data-accept="bin" onchange="qd.fileSizeCheck(this)"/>
                                </div>
                            </div>
                        </form>

                        <div class="clearfix" style="margin-top:1em; clear:both;text-align: center;">
                            <button type="button" class="btn btn-default" data-dismiss="modal">取消</button>
                            <a class="submit-version-release-btn btn btn-primary" style="padding:5px 15px;" data-loading-text="正在提交">
                                <i class="fa fa-check"> </i>
                                <span>提交</span>
                            </a>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>

    <div class="">
        <div class="pull-right">
            <a class="btn btn-default" href="javascript:" name="upgrade_btn" data-target="#upgrade_window" data-toggle="modal"><i class="glyphicon glyphicon-send"></i> 发布</a>
        </div>
        <br clear="all">
    </div>

    <br>
    <div class="data_list_div">
        <center></center>
    </div>
</div>